<?xml version="1.0"?>
<parameters>
    <dbtype>postgres</dbtype>
    <driver>org.postgresql.Driver</driver>
    <port>5433</port>
    <username>yugabyte</username>
    <DBName>yugabyte</DBName>
    <password></password>
    <isolation>TRANSACTION_REPEATABLE_READ</isolation>

    <sslCert></sslCert>
    <sslKey></sslKey>
    <jdbcURL></jdbcURL>

    <batchSize>128</batchSize>
    <useKeyingTime>false</useKeyingTime>
    <useThinkTime>false</useThinkTime>
    <enableForeignKeysAfterLoad>true</enableForeignKeysAfterLoad>
    <hikariConnectionTimeoutMs>180000</hikariConnectionTimeoutMs>
    <useStoredProcedures>true</useStoredProcedures>
    <displayEnhancedLatencyMetrics>true</displayEnhancedLatencyMetrics>
    <trackPerSQLStmtLatencies>true</trackPerSQLStmtLatencies>

   	<transactiontypes>
        <transaction>
            <name>NewOrder</name>
            <weight>1</weight>
        </transaction>
        <transaction>
            <name>Payment</name>
            <weight>0</weight>
        </transaction>
        <transaction>
            <name>OrderStatus</name>
            <weight>0</weight>
        </transaction>
        <transaction>
            <name>Delivery</name>
            <weight>0</weight>
        </transaction>
        <transaction>
            <name>StockLevel</name>
            <weight>0</weight>
        </transaction>
   	</transactiontypes>

    <runtime>30</runtime>
    <rate>500</rate>
    <!--
      Set the number of retries to 0 as retrying when the number of warehouses is
      high is pointless as it just leads to more failures.
    -->
    <maxRetriesPerTransaction>0</maxRetriesPerTransaction>
    <maxLoaderRetries>2</maxLoaderRetries>

</parameters>
